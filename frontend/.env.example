# Frontend Environment Variables
# Copy this to .env and fill in your values

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_WS_URL=ws://localhost:3001

# Casper Network
VITE_CASPER_NETWORK=casper-test
VITE_CASPER_RPC_URL=https://rpc.testnet.casperlabs.io/rpc

# Smart Contract Addresses (after deployment)
VITE_VAULT_MANAGER_HASH=
VITE_LIQUID_STAKING_HASH=
VITE_STRATEGY_ROUTER_HASH=
VITE_YIELD_AGGREGATOR_HASH=
VITE_CV_CSPR_TOKEN_HASH=
VITE_LST_CSPR_TOKEN_HASH=

# Feature Flags
VITE_ENABLE_THREE_JS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_TESTNET_BANNER=true

# Monitoring
VITE_SENTRY_DSN=
VITE_GA_TRACKING_ID=

# App Configuration
VITE_APP_NAME=CasperVault
VITE_APP_VERSION=1.0.0
